<%= server_to_mon %>,C Partition,check_snmp_storage.pl -H <%= server_to_mon %> -2 -C public -T pu -w 90% -c 95% -m C: -f
<%= server_to_mon %>,D Partition,check_snmp_storage.pl -H <%= server_to_mon %> -2 -C public -T pu -w 95% -c 99% -m D: -f
<%= server_to_mon %>,E Partition,check_snmp_storage.pl -H <%= server_to_mon %> -2 -C public -T pu -w 95% -c 99% -m E: -f
<%= server_to_mon %>,Current load,check_snmp_load.pl -H <%= server_to_mon %> -C public -2 -w 98 -c 99 -f
<%= server_to_mon %>,Memory Usage,check_snmp_storage.pl -H <%= server_to_mon %> -2 -C public -T bl -w 10 -c 5 -m  -f -q Ram
<%= server_to_mon %>,Parent slave,check_myslave.sh
