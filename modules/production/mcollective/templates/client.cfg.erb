# Basic stuff
topicprefix = <%= mc_topicprefix %>
libdir      = <%= mc_libdir %>
loglevel    = <%= mc_loglevel %>

# Plugins
securityprovider = <%= mc_security_provider_real %>
plugin.psk       = <%= mc_security_psk_real %>

# Middleware
connector = activemq
plugin.activemq.pool.size = 1
plugin.activemq.pool.1.host = <%= stomp_server %>
plugin.activemq.pool.1.port = <%= stomp_port %>
plugin.activemq.pool.1.user = <%= stomp_user %>
plugin.activemq.pool.1.password = <%= stomp_passwd %>
